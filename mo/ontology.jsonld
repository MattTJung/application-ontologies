  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "Secondary Ion Mass Spectrometry"
  }, {
    "@language" : "de",
    "@value" : "Sekundärionen-Massenspektrometrie"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
