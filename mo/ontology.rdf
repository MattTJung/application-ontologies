        <rdfs:label xml:lang="de">Aberration</rdfs:label>
        <rdfs:label xml:lang="en">Aberration</rdfs:label>
        <skos:definition xml:lang="de">Abweichung vom idealen oder erwarteten Verhalten, häufig im Zusammenhang mit optischen Systemen verwendet.</skos:definition>
