        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/mo"/>
        <rdfs:label xml:lang="en">Annular Dark Field Scanning Transmission Electron Microscopy</rdfs:label>
        <rdfs:label xml:lang="de">Ringförmige Dunkelfeld-Rastertransmissionselektronenmikroskopie</rdfs:label>
        <skos:definition xml:lang="en">ADF STEM is a technique that utilizes a specific arrangement of detectors to create contrast in STEM images based on the scattered electrons.</skos:definition>
        <co:definitionSource xml:lang="en">ChatGPT 3.5</co:definitionSource>
        <mo:abbreviation xml:lang="en">ADF-STEM</mo:abbreviation>
    </owl:Class>
    


    <!-- https://w3id.org/pmd/mo/Anode -->


