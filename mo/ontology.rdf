        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/mo"/>
        <rdfs:label xml:lang="en">Annular Bright Field Scanning Transmission Electron Microscopy</rdfs:label>
        <rdfs:label xml:lang="de">Ringförmige Hellfeld-Rastertransmissionselektronenmikroskopie</rdfs:label>
        <skos:definition xml:lang="en">ABF STEM is a technique that utilizes a specific arrangement of detectors to enhance contrast in STEM images, particularly for heavy elements.</skos:definition>
        <co:definitionSource xml:lang="en">ChatGPT 3.5</co:definitionSource>
        <mo:abbreviation xml:lang="en">ABF-STEM</mo:abbreviation>
    </owl:Class>
    


